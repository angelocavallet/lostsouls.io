  "scripts": {
    "docker-build": "docker build -t angelocavalet/lostsouls-game-server .",
    "docker-run": "docker run -d -t -p 8080:41500 angelocavalet/lostsouls-game-server",
    "docker-stop": "docker stop $(docker container ls -q)",
    "docker-reload": "npm run docker-stop \n npm run docker-build \n npm run docker-run",

    "babel-node": "babel-node --presets=@babel/preset-flow",
    "start": "nodemon --exec npm run babel-node -- ./app/app.js"

  },
  "dependencies": {
    "@babel/cli": "^7.20.7",
